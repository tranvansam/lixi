{
  "name": "myapp",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint .",
    "mock:server": "node mock/server.js",
    "cap:sync": "cap sync android",
    "cap:open": "cap open android",
    "android": "npm run build && cap sync android && cap run android",
    "build:apk": "npm run build && npx cap sync android && cd android && gradlew.bat assembleDebug",
    "build:apk:release": "npm run build && npx cap sync android && cd android && gradlew.bat assembleRelease",
    "build:apk:internal": "npm run build:apk && node scripts/copy-apk-internal.js"
  },
  "dependencies": {
    "@capacitor/android": "7.4.4",
    "@capacitor/app": "7.1.0",
    "@capacitor/core": "7.4.4",
    "@capacitor/device": "^7.0.2",
    "@capacitor/haptics": "7.0.2",
    "@capacitor/keyboard": "7.0.3",
    "@capacitor/status-bar": "7.0.3",
    "@ionic/vue": "^8.0.0",
    "@ionic/vue-router": "^8.0.0",
    "axios": "^1.7.7",
    "firebase": "^12.8.0",
    "google-tts-api": "^2.0.2",
    "html5-qrcode": "^2.3.8",
    "ionicons": "^7.0.0",
    "js-cookie": "^3.0.5",
    "pinia": "^3.0.4",
    "qrcode": "^1.5.4",
    "vee-validate": "^4.15.1",
    "vue": "^3.3.0",
    "vue-i18n": "^9.14.5",
    "vue-qrcode-reader": "^5.7.3",
    "vue-router": "^4.2.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.4",
    "@types/js-cookie": "^3.0.6",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "json-server": "^0.17.4",
    "sass": "^1.94.1",
    "terser": "^5.4.0",
    "typescript": "~5.9.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.1.10"
  },
  "description": "An Ionic project"
}
